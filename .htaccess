php_flag display_startup_errors on
php_flag display_errors on
php_flag html_errors on
php_flag log_errors on
php_flag ignore_repeated_errors off
php_flag ignore_repeated_source off
php_value error_reporting 2047
php_value error_log /home/chrise/www_root/local.b19.www/swish-katalogen/httpd-error.log
php_value log_errors_max_len 52428800


#php_value zlib.output_compression off

ServerSignature Off
FileEtag MTime

# Options +Indexes +FollowSymLinks
Options -MultiViews +FollowSymLinks

RewriteEngine On
RewriteBase /swish-katalogen

# remove www...
RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ https://%1/swish-katalogen/$1 [R=301,L]


# Top page
RewriteRule ^$ index.php [L]

# Sitemap
RewriteRule ^sitemap\x2exml$ sitemap.php [L]

# Categories
RewriteRule ^k/(.*)$ categories.php?cat=$1 [L,NC]

# Details
RewriteRule ^(123\d{7})$ detail.php?number=$1 [L,NC]



